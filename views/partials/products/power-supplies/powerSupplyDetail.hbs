{{#with powerSupply}}
<div class="product-specs">
  <section class="products-specs-list">
    <h2 class="products-specs-title">energy</h2>
    <ul class="products-specs-list">
      <li class="products-spects-list-item"><strong>Voltage: </strong>{{powerSupply.energy.voltage}}</li>
    </ul>
  </section>
  <section class="product-specs-section">
    <h2 class="product-specs-title">Weight and dimensions</h2>
    <ul class="product-specs-list">
      <li class="product-specs-list-item"><strong>Width: </strong>{{powerSupply.weightAndDimensions.width}}</li>
      <li class="product-specs-list-item"><strong>Height: </strong>{{powerSupply.weightAndDimensions.height}}</li>
      <li class="product-specs-list-item"><strong>Depth: </strong>{{powerSupply.weightAndDimensions.depth}}</li>
      <li class="product-specs-list-item"><strong>Weight: </strong>{{powerSupply.weightAndDimensions.weight}}</li>
    </ul>
  </section>
</div>
{{/with}}